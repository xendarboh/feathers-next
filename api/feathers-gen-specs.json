{
  "options": {
    "ver": "1.0.0",
    "inspectConflicts": false,
    "semicolons": true,
    "freeze": [],
    "ts": false
  },
  "app": {
    "environmentsAllowingSeedData": "test",
    "seedData": true,
    "name": "feathers-next-api",
    "description": "Project feathers-next-api",
    "src": "src",
    "packager": "yarn@>= 0.18.0",
    "providers": [
      "rest",
      "socketio"
    ]
  },
  "services": {
    "users": {
      "name": "users",
      "nameSingular": "user",
      "subFolder": "",
      "fileName": "users",
      "adapter": "nedb",
      "path": "/users",
      "isAuthEntity": true,
      "requiresAuth": true,
      "graphql": true
    },
    "counters": {
      "name": "counters",
      "nameSingular": "counter",
      "subFolder": "",
      "fileName": "counters",
      "adapter": "generic",
      "path": "/counters",
      "isAuthEntity": false,
      "requiresAuth": true,
      "graphql": true
    }
  },
  "hooks": {},
  "authentication": {
    "strategies": [
      "local"
    ],
    "entity": "users"
  },
  "connections": {
    "nedb": {
      "database": "nedb",
      "adapter": "nedb",
      "connectionString": "nedb://../data"
    }
  }
}
